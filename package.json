    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.13.9",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "passport": "^0.4.0"
  },
  "devDependencies": {
    "nodemon": "^1.14.10"
  }
}
